-- Create table
create table T_SUPPLIER_INFO_CTL
(
  ctl_id       VARCHAR2(32) not null,
  itf_id       VARCHAR2(32) not null,
  itf_type     VARCHAR2(32) not null,
  batch_id     VARCHAR2(32),
  batch_num    NUMBER,
  batch_time   VARCHAR2(32),
  sender       VARCHAR2(32),
  receiver     VARCHAR2(32),
  send_time    DATE,
  receive_time DATE,
  return_type  VARCHAR2(32),
  return_msg   VARCHAR2(256),
  create_id    VARCHAR2(32),
  create_time  DATE,
  update_id    VARCHAR2(32),
  update_time  DATE,
  company_id   VARCHAR2(32)
)
tablespace SCMDATA
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
nologging;
