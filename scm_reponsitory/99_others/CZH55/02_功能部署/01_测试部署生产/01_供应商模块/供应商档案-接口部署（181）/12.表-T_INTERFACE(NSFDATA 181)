-- Create table
create table T_INTERFACE
(
  interface_id          VARCHAR2(32) not null,
  dock_item_id          VARCHAR2(32),
  ctl_item_id           VARCHAR2(32),
  ds_item_id            VARCHAR2(32),
  pause                 NUMBER,
  dock_item_caption     VARCHAR2(48),
  ctl_item_caption      VARCHAR2(48),
  ds_item_caption       VARCHAR2(48),
  company_id            VARCHAR2(48),
  itf_name              VARCHAR2(48),
  error_resolve_caption VARCHAR2(48),
  error_resolve_item_id VARCHAR2(48)
)
tablespace NSFDATA
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
nologging;
-- Add comments to the columns 
comment on column T_INTERFACE.error_resolve_caption
  is '错误处理';
comment on column T_INTERFACE.error_resolve_item_id
  is '错误处理';
-- Create/Recreate primary, unique and foreign key constraints 
alter table T_INTERFACE
  add constraint PK_INTERFACE_ID primary key (INTERFACE_ID)
  using index 
  tablespace NSFDATA
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
alter index PK_INTERFACE_ID nologging;
